/**list.wxss**/
.list_container{
  display: flex;
  flex-direction: column;
  padding: 0px 5px;
}
.list_Title{
  color: #353535;
  font-size: 17pt;
  padding: 20px 0px 15px 10px;
}
.list_item{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: space-between;
  border-bottom: 1px solid #eeeeee;
  padding: 12px;

}
.list_item_title{
  color: #888888;
  font-size: 12pt;
}
