/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.text-gray{
  color: #888;
}


.list_container{
  display: flex;
  flex-direction: column;
  padding: 0px 5px;
}
.list_Title{
  color: #353535;
  font-size: 17pt;
  padding: 20px 0px 15px 10px;
}
.list_item{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: space-between;
  border-bottom: 1px solid #eeeeee;
  padding: 12px;

}
.list_item_title{
  color: #888888;
  font-size: 12pt;
}
.bg_info_color{
  background-color: #5bc0de;
}
.bg_primary_color{
  background-color: #337ab7;
}
.bg_danger_color{
  background-color: #d9534f;
}
.bg_warnning_color{
  background-color: #f0ad4e;
}
.bg_success_color{
  background-color: #5cb85c;
}


